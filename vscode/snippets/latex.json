{
	// Quicker \item
	"Item": {
		"prefix": "item",
		"body": "\\item $1",
		"description": "Enumeration, description and itemize item"
	},
	// Snippets for custom cookbook class
	"Ingredients List": {
		"prefix": "ingredient",
		"body": [
			"\\begin{ingredients}",
			"\t\\item $1",
			"\\end{ingredients}"
		],
		"description": "List of ingredients for a recipe"
	},
	"Recipe Directions": {
		"prefix": "directions",
		"body": [
			"\\begin{directions}",
			"\t\\item $1",
			"\\end{directions}"
		],
		"description": "List of directions for recipe"
	},
	"Date": {
		"prefix": "date",
		"body": [
			"$1\\textsuperscript{$2} of $3, $CURRENT_YEAR"
		],
		"description": "Enter current date"
	},
	"Cookbook Fraction": {
		"prefix": "cgfrac",
		"body": [
			"\\cgfrac[$1]{$2}{$3}"
		],
		"description": "Custom fraction typesetting for cookbooks"
	},
    "minted-environment": {
        "prefix": "minted",
        "body": [
            "\\begin{minted}{$1}",
            "\t$2",
            "\\end{minted}"
        ],
        "description": "Minted code highlighting environment"
    }
}
